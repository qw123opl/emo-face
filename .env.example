# LLM API Configuration
LLM_API_KEY=""

# LLM Provider Configuration
LLM_PROVIDER=openai

# Model Configuration (根據 provider 選擇對應模型)
# OpenAI 可用模型: gpt-4o-mini, gpt-4o, gpt-4-turbo, gpt-3.5-turbo
# Gemini 可用模型: gemini-pro, gemini-1.5-pro
OPENAI_MODEL=gpt-4.1-nano
GEMINI_MODEL=gemini-pro

# Application Configuration
FLASK_ENV=development
FLASK_DEBUG=True
PORT=8080
